<table class="table table-striped table-bordered table-custom" cellspacing="0">
  <%= link_to t("category_admin.create"), new_admin_category_path, remote: true,
    class: "btn btn-info btn-lg" %>
  <thead>
    <tr>
      <th><%= t "category_admin.name" %></th>
      <th class = "last-column"><%= t "category_admin.action" %></th>
    </tr>
  </thead>
  <tbody class="category-index">
    <%= render "list" %>
  </tbody>
  <div id="category-modal" class="modal fade"></div>
</table>
