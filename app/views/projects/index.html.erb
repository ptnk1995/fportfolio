<section id="page-header">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="section-title">
          <h1><%= t "portfolio" %></h1>
          <span class="st-border"></span>
        </div>
      </div>
    </div>
  </div>
</section>
<div id="portfolio" class="bg-light-gray">
  <div class="container">
    <div class="row">
      <%= render @projects %>
    </div>
    <div class="center">
      <%= paginate @projects, theme: "twitter-bootstrap-3" %>
    </div>
  </div>
</div>
<div class="portfolio-show"></div>
